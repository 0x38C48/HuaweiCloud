.container {
  padding: 20px;
  font-family: Arial, sans-serif;
}

.title {
  font-size: 18px;
  margin-bottom: 10px;
}

.device {
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin: 5px 0;
}

.device button {
  margin-left: 5px;
  padding: 5px 10px;
  background-color: #1aad19;
  color: white;
  border: none;
  border-radius: 3px;
}
